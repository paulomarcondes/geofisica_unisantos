@article{collettini_normal_2001,
	title = {Normal faults, normal friction?},
	volume = {29},
	url = {http://geology.gsapubs.org/cgi/content/abstract/29/10/927-a},
	doi = {10.1130/0091-7613(2001)029<0927:NFNF>2.0.CO;2},
	abstract = {Debate continues as to whether normal faults may be seismically active at very low dips (\{delta\} {\textless} 30\{degrees\}) in the upper continental crust. An updated compilation of dip estimates (n = 25) has been prepared from focal mechanisms of shallow, intracontinental, normal-slip earthquakes (M {\textgreater} 5.5; slip vector raking 90\{degrees\} \{+/-\} 30\{degrees\} in the fault plane) where the rupture plane is unambiguously discriminated. The dip distribution for these moderate-to-large normal fault ruptures extends from 65\{degrees\} {\textgreater} \{delta\} {\textgreater} 30\{degrees\}, corresponding to a range, 25\{degrees\} {\textless} \{theta\}r {\textless} 60\{degrees\}, for the reactivation angle between the fault and inferred vertical \{sigma\}1. In a comparable data set previously obtained for reverse fault ruptures (n = 33), the active dip distribution is 10\{degrees\} {\textless} \{delta\} = \{theta\}r {\textless} 60\{degrees\}. For vertical and horizontal \{sigma\}1 trajectories within extensional and compressional tectonic regimes, respectively, dip-slip reactivation is thus restricted to faults oriented at \{theta\}r [≤] 60\{degrees\} to inferred \{sigma\}1. Apparent lockup at \{theta\}r \{approx\} 60\{degrees\} in each dip distribution and a dominant 30\{degrees\} \{+/-\} 5\{degrees\} peak in the reverse fault dip distribution, are both consistent with a friction coefficient \{micro\}s \{approx\} 0.6, toward the bottom of Byerlee's experimental range, though localized fluid overpressuring may be needed for reactivation of less favorably oriented faults.},
	number = {10},
	urldate = {2010-06-23},
	journal = {Geology},
	author = {Collettini, Cristiano and Sibson, Richard H.},
	year = {2001},
	pages = {927--a-930}
},

