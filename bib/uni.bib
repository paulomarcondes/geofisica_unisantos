@article{lucier_constraining_2009,
	title = {Constraining the far-field in situ stress state near a deep South African gold mine},
	volume = {46},
	issn = {1365-1609},
	url = {http://www.sciencedirect.com/science/article/B6V4W-4TX6WB1-1/2/4ce561fbc54306d5350a5321cdd23488},
	doi = {10.1016/j.ijrmms.2008.09.005},
	abstract = {We present and test a new technique for determining the far-field virgin state of stress near the {TauTona} gold mine. The technique we used to constrain the far-field stress state follows an iterative forward modelling approach that combines observations of drilling-induced borehole failures in borehole images, boundary element modelling of the mining-induced stress perturbations, and forward modelling of borehole failures based on the results of the boundary element modelling. Using this approach, we constrained a range of principal stress orientations and magnitudes that are consistent with all the observed failures and other stress indicators. We found that the state of stress is a normal faulting regime (Sv*[greater-or-equal, {slanted]SHmax*[greater-or-equal}, {slanted]Shmin*)} with principal stress orientations that are slightly deviated from vertical and horizontal and, therefore, denoted with a (*). The maximum principal stress, Sv*, is deviated {\textasciitilde}10° from vertical plunging towards the {NNW} with a magnitude gradient of {\textasciitilde}27 {MPa/km.} The intermediate principal stress, {SHmax*}, is inclined {\textasciitilde}10° from horizontal plunging towards an azimuth of {\textasciitilde}156° and has a magnitude gradient of {\textasciitilde}24 {MPa/km.} The least principal stress, Shmin*, is inclined {\textasciitilde}5° from horizontal plunging towards an azimuth of 247° and has a magnitude gradient of {\textasciitilde}14 {MPa/km.} This stress state indicates that the crust is in a state of frictional faulting equilibrium, such that normal faulting is likely to occur on cohesionless pre-existing fault planes that are optimally oriented to the stress field. Modelling of breakout rotations and gaps in breakout occurrence associated with recent fault slip on critically stressed faults located {\textgreater}100 m from the mine further confirmed this stress state.},
	number = {3},
	urldate = {2010-06-23},
	journal = {International Journal of Rock Mechanics and Mining Sciences},
	author = {Lucier, Amie M. and Zoback, Mark D. and Heesakkers, Vincent and Reches, Ze'ev and Murphy, Shaun K.},
	year = {2009},
	keywords = {Boundary element modelling, Breakouts, Drilling-induced tensile fractures, in situ stress, Induced micro-seismicity, Mines},
	pages = {555--567}
},

