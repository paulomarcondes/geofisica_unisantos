@article{scott_wilkerson_factors_1991,
	title = {Factors controlling slip-lineation orientation on thrust-fault planes},
	volume = {196},
	issn = {0040-1951},
	url = {http://www.sciencedirect.com/science/article/B6V72-48B5G7S-3C/2/9dfd09956f56ac08620cd531a5e932d9},
	doi = {10.1016/0040-1951(91)90297-6},
	abstract = {The presence of multiple sets of curved and/or non-coaxial slip lineations on a single fault surface suggests changing stress conditions during the movement history of the fault. Curved slip lineations suggest a progressive change in the stress field during a single displacement event, whereas non-coaxial slip lineations suggest periodic reactivation of a fault under different stress conditions. If the direction of slip is assumed to reflect the direction of maximum shear stress acting on a pre-existing fault, changes in only the relative magnitudes of principal stresses acting on the thrust sheet are sufficient to cause changes in slip-lineation orientation. Consequently, curved and/or non-coaxial slip lineations simply may record temporal changes in principal-stress magnitudes resulting from syntectonic erosion, changes in tectonic-stress magnitude, and ramping of thrusts to shallower depths. We illustrate that a decrease in overburden or an increase in the magnitude of tectonic stress results in a progressive up-dip rotation of slip lineations on a fault surface, a rotation which could be misinterpreted as indicating polyphase deformation caused by changes in regional displacement directions. Changes in pore pressure do not influence the trajectory of slip on a fault surface, but rather affect the conditions governing the likelihood for slip to occur.},
	number = {1-2},
	urldate = {2010-06-24},
	journal = {Tectonophysics},
	author = {Scott Wilkerson, M. and Marsha, Stephen},
	year = {1991},
	pages = {203--208}
},

