	note = {{PMID:} 23038986},
	keywords = {Animals, Behavior, Animal, Cell Differentiation, Cicatrix, Exploratory Behavior, Ketogenic Diet, Leukocyte Count, Leukocytes, Male, Maze Learning, Models, Animal, Motor Activity, Rats, Rats, Wistar, Weight Gain},
