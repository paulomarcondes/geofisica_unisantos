@article{lubbe_field_2006,
	title = {A field investigation of fracture compliance},
	volume = {54},
	issn = {1365-2478},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2478.2006.00530.x/abstract},
	doi = {10.1111/j.1365-2478.2006.00530.x},
	abstract = {A field measurement of fracture compliance is described. The aim was to determine how compliance scales with fracture size and, specifically, how laboratory measurements of fracture compliance compared with field estimates from sonic and seismic data. A test site was constructed, consisting of three 40 m vertical boreholes drilled in the floor of a Carboniferous Limestone quarry. Detailed knowledge of the rocks in the test area was obtained from core analysis, wireline logging and local area fracture mapping. Seismic cross-hole surveys were performed using a sparker source with a dominant frequency of 2000 Hz and hydrophone receivers. The rocks had a compressional-wave velocity anisotropy of 10\%, which was attributed to the presence of predominantly horizontal, partially open fractures. Estimates of normal fracture compliance within a range from 2.5 × 10−13 {m/Pa} to 3.5 × 10−12 {m/Pa} were obtained from both the cross-hole data and the sonic-log data. This is an order of magnitude greater than values obtained from laboratory experiments which are reported elsewhere.},
	language = {en},
	number = {3},
	urldate = {2011-06-01},
	journal = {Geophysical Prospecting},
	author = {Lubbe, R. and Worthington, M. H},
	month = may,
