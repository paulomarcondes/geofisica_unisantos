@inproceedings{bednar_full_2005,
	title = {Full azimuth from narrow azimuth via {AMO}},
	volume = {24},
	doi = {10.1190/1.2148075},
	abstract = {Mapping seismic data from one acquisition geometry to another is an extremely useful technology. There are several reasonably well known proposed solutions to this problem. These include offerings from Canning and Gardner (Canning and {Gardner,1996),Bleistein} (Bleistein and Jaramillo, 2000), Stolt (Stolt, 2002), Biondi, Fomel, and Chemingue (Biondi et al.,2001; Fomel and {Biondi,1995;Chemingui} and Biondi,1995; Biondi and {Chemingui,1994;Fomel,2003).All} of these articles provide at least one method for mapping input data with arbitrary source-receiver azimuths to data with a fixed source-receiver azimuth. Because it is at least partially based on knowledge of a velocity model for imaging, Bleistein methodology requires the generation of traveltimes and consequently requires a reasonably accurate interval velocity model. Stolt's method circumvents this problem by avoiding the depth domain. The approach developed by Biondi, Fomel, and Chemingue, which I will call integral {AMO} and is based only on normal moveout velocities, offers the potential for an efficient easily parallelized space-time algorithm. In contrast, the method of Canning and Gardner, which I will call F-K {AMO}, is completely velocity {independent.A} primary reason for interest in data mapping exercises is the fact that many modern imaging and coherent noise (e.g. multiple) suppression algorithms demand a specific acquisition geometry. Such requirements range from common-azimuth to full azimuth and full offset geometries. Having a specific geometry allows one to formulate a specific algorithm whose efficiency is dramatically improved by the fact that the data has a specific order or style. Increased efficiency relates directly to increased speed and turnaround and is certainly desirable in a modern fast-paced exploration {setting.In} this article, I investigate the extent to which full azimuth data can be produced from relatively narrow-azimuth data. While I focus on integral and F-K {AMO} the fundamental idea is simple. Instead of trying to generate a full areal shot at each and every location, I generate multiple fixed offset and azimuth volumes. Extracting a fixed receiver ensemble from each of these volumes creates, by source-receiver reciprocity, a full azimuth areal shot. While the implementation of the formulas of integral {AMO} can be more difficult then the approach of Canning and Gardner, the somewhat better amplitude control of integral {AMO} tends to produce a better result. Amplitude control is a critical aspect of data mapping, so it also becomes a key part of the testing and evaluation process. Because of certain potential irregularities in the formulation of integral {AMO}, it is often not easy to achieve.},
	booktitle = {{SEG} Technical Program Expanded Abstracts},
	author = {Bednar, J. Bee and Wang, Yongzhong},
	month = jan,
	year = {2005},
	pages = {1898--1901}
},

@article{zhang_dual-porosity_2003,
	title = {Dual-porosity poroelastic analyses of wellbore stability},
	volume = {40},
	issn = {1365-1609},
	url = {http://www.sciencedirect.com/science/article/B6V4W-48CGG9W-3/2/2603000e212a03460e74f3d6f5ba6c44},
	doi = {10.1016/S1365-1609(03)00019-4},
	abstract = {This paper presents a numerical solution of dual-porosity poroelastic formulations that couple solid deformations with fluid flow in both matrix and fracture systems of naturally fractured reservoirs. The finite element numerical method was applied to solve the inclined wellbore problem in which mud weight was considered for both permeable and impermeable boundary conditions. Failure criteria relevant to tensile, collapse, and shear failure modes were introduced into the numerical model and resulting failure areas around boreholes were subsequently defined. Several related applications, including the stability of inclined and horizontal wellbores under various in situ stress regimes, were evaluated. Upper and lower mud weight bounds and the most stable borehole orientations were determined. The best trajectory selections for horizontal boreholes were also investigated.},
	number = {4},
	urldate = {2010-06-24},
	journal = {International Journal of Rock Mechanics and Mining Sciences},
	author = {Zhang, Jincai and Bai, Mao and Roegiers, J. -C.},
	month = jun,
	year = {2003},
	pages = {473--483}
},

@article{haimson_deep_1975,
	title = {Deep in-situ stress measurements by hydrofracturing},
	volume = {29},
	issn = {0040-1951},
	url = {http://www.sciencedirect.com/science/article/B6V72-48BDP06-W8/2/0fa0015a8be9bfedd594b6b080b28a09},
	doi = {10.1016/0040-1951(75)90131-6},
	abstract = {Hydrofracturing is a recently developed in-situ stress-measuring method. It consists of hydraulically pressurizing a sealed-off interval inside of a borehole until fracture initiates. The fracture is then extended by additional pumping of fluids from the surface. The pressures recorded during the test can be directly related to the magnitudes of the in-situ principal stresses. The orientation of the fracture in the borehole yields the directions of the principal stresses. The method overcomes some limitations of the more conventional strain-relief techniques: stresses can be determined at any distance from the access point (limited only by hole length), stresses are measured directly, no elastic parameters are required in determining the minimum principal stress, no delicate instruments are used in the borehole, no overcoring is necessary. The hydrofracturing method was theoretically developed for both high- and low-permeability rocks, and was extensively tested in the laboratory under simulated in-situ conditions. These tests confirmed the theoretical assumptions and results. Field measurements were conducted in some ten states within the continental {U.S.}, at depths varying from 100 m (Nevada Test Site) to 1900 m (Rangely, Colorado). The hydrofracturing results were very consistent within each site, and the calculated principal stresses and their directions could, in most cases, be correlated to tectonic phenomena or other measurements and observations. In particular, the stresses determined at Rangely were in accord with the expected condition for the right lateral slip of the existing strike-slip fault, and were used to correctly predict the critical pore pressure necessary to trigger local earthquakes. At the Nevada Test Site, the results of twelve hydrofracturing measurements conducted both from the surface and from underground tunnels indicated a linear increase with depth in all the principal stresses. Hydrofracturing is a simple method of stress determination. It utilizes commercially available packers, pressure lines and pumps, which can be either rented or purchased. It is now being tested in fault zones, in anomalously hot formations, in extremely anisotropic rocks. It is being considered as a major tool in plate-tectonics studies, earthquake control, geothermal-energy extraction from hot dry rock, and new methods of mining.},
	number = {1-4},
	urldate = {2010-06-24},
	journal = {Tectonophysics},
	author = {Haimson, Bezalel C.},
	year = {1975},
	pages = {41--47}
},

