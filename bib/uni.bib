@inproceedings{martin_integrated_1986,
	title = {An integrated three-component approach to fracture detection},
	volume = {1986},
	url = {http://www.geoscienceworld.org/cgi/georef/georef;2001043396},
	doi = {10.1190/1.1893146},
	abstract = {Because compressional-waves are relatively insensitive to fractures, conventional P-wave seismic surveys are generally not well suited to detecting the presence of fractures in the subsurface. However, recent theoretical and experimental work has shown that shear waves are sensitive to the presence and orientation of fractures. Subsurface fracture systems normally consist of nearly vertical, aligned fractures, creating a zone of effective anisotropy with an axis of symmetry in a near horizontal plane. Shear waves entering such a zone necessarily split into two or more phases with different velocities and polarization. A number of investigators have suggested that this shear-wave splitting and polarization could be recorded and interpreted by recording all three components of wave motion. To test this hypothesis, the Dept. of Geophysics at Colorado School of Mines acquired 5 miles of 3-component P and {SH-wave} surface seismic Vibroseis data over a known fractured reservoir. In addition, 3-component P and {SH-wave} {VSPs} were recorded in a nearby well. Funding for this research was provided by a consortium of 13 industrial sponsors and data acquisition was completed in November 1985.},
	booktitle = {{SEG} Technical Program Expanded Abstracts},
	author = {Martin, M. A. and Davis, T. L. and {O'Rourke}, T. J.},
	month = jan,
	year = {1986},
	keywords = {{3C}, Anisotropy, body waves, data acquisition, data processing, detection, elastic waves, fractured reservoirs, fractures, geophysical methods, P-waves, polarization, S-waves, seismic methods, seismic waves, seismographs, Vibroseis, wave dispersion},
