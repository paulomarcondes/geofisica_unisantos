@article{bell_practical_2003,
	title = {Practical methods for estimating in situ stresses for borehole stability applications in sedimentary basins},
	volume = {38},
	issn = {0920-4105},
	url = {http://www.sciencedirect.com/science/article/B6VDW-486G7K2-3/2/378ac6206cf5395deb93db5a2d28bf86},
