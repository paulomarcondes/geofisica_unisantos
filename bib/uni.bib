	journal = {International Journal of Rock Mechanics and Mining Sciences},
	author = {Mas Ivars, Diego and Pierce, Matthew E. and Darcel, Caroline and Reyes-Montes, Juan and Potyondy, David O. and Paul Young, R. and Cundall, Peter A.},
