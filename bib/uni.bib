@article{sibson_note_1985,
	title = {A note on fault reactivation},
	volume = {7},
	issn = {0191-8141},
	url = {http://www.sciencedirect.com/science/article/B6V9D-489RX3S-4S/2/e7f7cce8678251cfffbd08febfec7e96},
	doi = {10.1016/0191-8141(85)90150-6},
	abstract = {Reactivation of existing faults whose normal lies in the [sigma]1'[sigma]3' plane of a stress field with effective principal compressive stresses [sigma]1' {\textgreater}[sigma]2' {\textgreater}[sigma]3' is considered for the simplest frictional failure criterion, [tau] = [mu][sigma]n' = [mu]([sigma]n - P), where [tau] and [sigma]n are respectively the shear and normal stresses to the existing fault, P is the fluid pressure and [mu] is the static friction. For a plane oriented at [theta] to [sigma]1', the stress ratio for reactivation is ([sigma]1'/[sigma]3') = (1 + [mu] cot [theta])/(1 - [mu] tan [theta]). This ratio has a minimum positive value at the optimum angle for reactivation given by (1/[mu]) but reaches infinity when [theta] = 2[theta]*, beyond which [sigma]3' {\textless} 0 is a necessary condition for reactivation. An important consequence is that for typical rock friction coefficients, it is unlikely that normal faults will be reactivated as high-angle reverse faults or thrusts as low-angle normal faults, unless the effective least principal stress is tensile.},
	number = {6},
	urldate = {2010-06-24},
	journal = {Journal of Structural Geology},
	author = {Sibson, Richard H},
	year = {1985},
	pages = {751--754}
},

