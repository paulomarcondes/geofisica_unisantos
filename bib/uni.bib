@article{mandl_tectonic_1987,
	title = {Tectonic deformation by rotating parallel faults: the "bookshelf" mechanism},
	volume = {141},
	issn = {0040-1951},
	shorttitle = {Tectonic deformation by rotating parallel faults},
	url = {http://www.sciencedirect.com/science/article/B6V72-48C7760-4X/2/50f0d6e7ea743e712c25a2b2c4f6f555},
	doi = {10.1016/0040-1951(87)90205-8},
	abstract = {In various tectonic environments, extension of crustal parts and shear distortion has been accommodated by the rotation of an array of parallel faults. Because of its kinematic resemblance to the tilting of a row of books on a shelf, the tectonic process may be referred to as a "bookshelf" mechanism. Its most important manifestions are cross-faulting between normal faults or parallel wrench faults, the extension of deltaic slope deposits and all forms of tilted block tectonics.
Kinematically, there are two basic modes of the mechanism: (a) the "dilational" mode, where rotating faults tend to open up and possibly become paths for fluid migration and (b) the "domino" style where reclining faults remain closed. In both cases, formation and rotation of the faults may take place in one tectonic event. Fault displacements may therefore often remain small and difficult to detect. From the mechanical point of view, bookshelf operations that accommodate shearing between parallel and predominantly parallel moving rock boundaries ([`]bookshelf' mechanisms proper) have to be differentiated from those operations responding to an imposed crustal extension or shortening.
After a review of geological evidence the process is addressed from a geomechanical point of view in order to determine the geological operating conditions and controlling parameters.
The crucial mechanical factor in the bookshelf mechanism is the component of normal stress that acts in the direction of the imposed displacement component parallel to the driving boundary plates. In contrast to the usual assumption, the driving shear stress on the boundary plates not only depends on the normal stress on the plates, but it also varies with the value of the normal stress that acts inside the sheared material in the direction of the plate-parallel displacement component. It is the relationship between the two normal stresses that also determines the type of rotating parallel faults.
Large-scale rotation of parallel normal faults in domino style (tilted block tectonics) is primarily associated with the extension of ductile substrata. From mechanical arguments and sand box experiments it has been inferred how the process and in particular, the dip direction of the parallel faults, is controlled by the way substratal extension progresses, by means of the direction of a substratal squeeze flow, the presence of a surface slope, the strength anisotropy associated with fore-set beds and by the configuration of competent boundaries of the extending overburden.},
	number = {4},
	urldate = {2010-06-24},
	journal = {Tectonophysics},
	author = {Mandl, G.},
	year = {1987},
	pages = {277--285, 289-316}
},

