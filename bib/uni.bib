@article{ahlgren_nucleation_2001,
	title = {The nucleation and evolution of Riedel shear zones as deformation bands in porous sandstone},
	volume = {23},
	issn = {0191-8141},
	url = {http://www.sciencedirect.com/science/article/B6V9D-43B293M-3/2/225831deb8d4a9f5e1ecd1b99c811977},
	doi = {10.1016/S0191-8141(00)00183-8},
	abstract = {The term Riedel shear zone refers to a geometric fracture pattern commonly associated with strike-slip fault systems. Using field mapping and three-dimensional digital modeling, the progressive temporal evolution of natural Riedel shear zones within exposures of the Jurassic Navajo Sandstone is interpreted from the spatial evolution of small-scale, incipient Proto-Riedel Zones ({PRZs)} to more completely developed systems. {PRZs} nucleate as a tabular zone of localized shearing marked by en échelon deformation bands, each of which is no more than a few mm wide and tens of cm long. These initial deformation bands have an opposite (antithetic) sense of slip with respect to the zone, and are oriented at an acute angle of 55-85° to the trend of the zone. With increasing strain, deformation bands and sedimentary markers become sheared through granular flow across the zone and assume a sigmoidal form. Subsequent bands develop as conjugate shear fractures within the strain-hardened axis of the {PRZ.} These antithetic driven systems are not directly linked to pre-existing or external structural elements and are not compatible with traditional synthetic driven models of Riedel shear zones.},
	number = {8},
	urldate = {2010-06-24},
	journal = {Journal of Structural Geology},
	author = {Ahlgren, S. G.},
	year = {2001},
	keywords = {Deformation band, Navajo sandstone, Riedel shear zone, Utah, Waterpocket fold},
	pages = {1203--1214}
},

@article{annavarapu_joint_2012,
