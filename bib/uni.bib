@article{assumpcao_focal_1998,
	title = {Focal mechanisms of small earthquakes in the southeastern Brazilian shield: a test of stress models of the South American plate},
	volume = {133},
	shorttitle = {Focal mechanisms of small earthquakes in the southeastern Brazilian shield},
	url = {http://dx.doi.org/10.1046/j.1365-246X.1998.00532.x},
	doi = {10.1046/j.1365-246X.1998.00532.x},
	abstract = {Focal mechanisms of small earthquakes with magnitudes of about 3 in the {SE} Brazilian shield are calculated using {S/P} amplitude ratios. Low attenuation (Qp from 400 to 800) in the shield upper-crustal layers allowed sharp S arrivals to be recorded up to distances of 100 km. Besides P-wave polarities, {SH-wave} first motions were also used to constrain the nodal-plane orientations. Normal and reverse faulting mechanisms with strike-slip components were found. The inversion of four mechanisms to estimate the stress tensor indicated a strike-slip stress regime with roughly {E2013W-orientated} {03C31} and {N2013S} {03C33.} Both the orientations and the shape factor ({03C6=0.7)} of the inverted stress are in excellent agreement with theoretical predictions for that part of Brazil from the driving-force model of Coblentz \& Richardson (1996). Good agreement with the nature of the stress, as well as its orientation, was also found for the model of Meijer (1995). Both of these theoretical models include spreading stresses along the continent/ocean lithospheric transition. Because the earthquakes are more than 300 km from the continental shelf they should not be affected by the local flexural forces caused by sediment load in the marginal basins. The agreement between observed and theoretical stresses then confirms the importance of continental spreading forces in modelling intraplate stresses.},
	number = {2},
	urldate = {2010-04-07},
	journal = {Geophysical Journal International},
	author = {Assumpção, Marcelo},
	year = {1998},
	keywords = {Brasil, earthquake, stress},
	pages = {490--498}
},

@inproceedings{becker_seismic_1986,
	title = {Seismic detection of subsurface fractures},
	volume = {1986},
	doi = {10.1190/1.1893040},
	abstract = {Seismically, an anisotropic medium is one in which the material properties governing elastic wave propagation vary with direction. The variation in these properties may result from crystalline anisotropy and/or preferred mineral alignment, a directional variation of isotropic properties due to an imposed stress differential, and/or a preferred fracture orientation (micro and/or macro) resulting in a quasi-elastic medium. In the case of fracture-induced anisotropy, as will be investigated here, the fractures cause a variation in P and S-wave velocities for propagation parallel and perpendicular to the fracture plane (similar to the ordinary and extraordinary rays in optically birefringent crystals). Hence, when a shear-wave enters a fractured media, the wave splits into two quasi-shear waves ({qSH} and {qSV).} These waves then propagate with different velocities and are polarized in different planes. Also, a quasi-P-wave {qP} is generated when a compressional wave propagates through a fractured medium. The direction of particle motion for the {qP} wave deviates from the propagation direction in all but the symmetry planes. Crampin (1978) and Crampin et al. (1980) used hodograms to identify shear-wave splitting resulting from propagation through an anisotropic media. A hodogram is a plot of the particle velocity in one direction versus that in another (usually orthogonal) direction as a function of time. From such a plot, the following information can be derived: (1) whether the particle motion is linearly or elliptically polarized; (2) in the case of elliptical polarization, what are the orientations of the principal symmetry planes; and (3) in the case of shear-wave splitting when the two velocities are known, what is the fracture orientation. To identify and describe fully wave polarization and splitting, a complete measure of the elastic wave field must be made. This is accomplished by using 3-component detectors to record the wave field. A 3-component {VSP} data set was recorded in central Texas. Analysis of elliptical particle motion measured in the {VSP} data indicated zones of possible anisotropy or fracturing in the subsurface. From the {VSP} data, orientation of the principal axes of anisotropy in the Austin and Buda units was determined.},
	booktitle = {{SEG} Technical Program Expanded Abstracts},
	author = {Becker, D. F. and Perelberg, A. I.},
	month = jan,
	year = {1986},
	keywords = {{3C}, Anisotropy, Austin Chalk, body waves, Buda Limestone, buried features, carbonate rocks, chalk, Comanchean, Cretaceous, Cretaceous, Upper, detection, elastic waves, fractures, geophysical methods, geophysical profiles, geophysical surveys, Gulfian, limestone, Mesozoic, polarization, propagation, rock mechanics, S-waves, sediment, seismic methods, seismic profiles, seismic waves, seismographs, surveys, United States, {US-Texas}, {VSP}, wave splitting},
	pages = {466–468}
},

